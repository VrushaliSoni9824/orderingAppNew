<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@android:color/transparent"
    android:orientation="vertical"
    tools:ignore="MissingDefaultResource">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/_8sdp"
        android:layout_marginEnd="@dimen/_8sdp"
        android:layout_marginBottom="@dimen/_4sdp"
        android:background="@drawable/bg_white_drawer_layout"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:srcCompat="@android:drawable/sym_def_app_icon" />

        <TextView
            android:id="@+id/textViewOwnerName"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_gravity="center_vertical"
            android:gravity="center"
            android:text="Andre"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="@color/orange"
            android:textSize="@dimen/_15ssp"
            android:textStyle="bold" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/_6sdp"
            android:background="@drawable/gray_border"
            android:elevation="@dimen/_3sdp"
            android:gravity="center"
            android:orientation="vertical"
            android:padding="@dimen/_15sdp"
            app:cardElevation="@dimen/_3sdp">

            <Switch
                android:id="@+id/switchBusiness"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen_50dp"
                android:layout_centerVertical="true"
                android:layout_gravity="center"
                android:checked="true"
                android:text="Business name"
                android:textColor="@color/orange"
                android:textSize="@dimen/_15ssp"
                android:textStyle="bold"
                android:thumb="@drawable/bg_switch"
                android:track="@drawable/custom_track"
                tools:ignore="UseSwitchCompatOrMaterialXml" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/navigation_online_order"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:elevation="@dimen/_3sdp"
            android:background="@drawable/bg_grey_drawer_layout_1"
            android:layout_margin="@dimen/_6sdp"
            app:cardElevation="@dimen/_3sdp"
            android:padding="@dimen/_7sdp">

            <ImageButton
                android:id="@+id/listIcon"
                android:layout_width="@dimen/_30sdp"
                android:layout_height="@dimen/_30sdp"
                android:layout_centerVertical="true"
                android:layout_marginRight="16dp"
                android:padding="@dimen/_5sdp"
                android:background="@drawable/bg_grey_drawer_layout_2"
                android:elevation="@dimen/_5sdp"
                android:src="@drawable/ic_restaurant_menu"
                android:tint="@color/orange" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_gravity="center_vertical"
                android:text="@string/DoneOrder"
                android:textStyle="bold"
                android:textSize="@dimen/_15ssp"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="@color/orange" />


        </LinearLayout>

        <LinearLayout
            android:id="@+id/navigation_pos"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/_6sdp"
            android:layout_marginTop="@dimen/_4sdp"
            android:layout_marginBottom="@dimen/_4sdp"
            android:background="@drawable/bg_grey_drawer_layout_1"
            android:elevation="@dimen/_3sdp"
            android:orientation="horizontal"
            android:padding="@dimen/_7sdp"
            android:visibility="gone">

            <ImageButton
                android:layout_width="@dimen/_30sdp"
                android:layout_height="@dimen/_30sdp"
                android:layout_centerVertical="true"
                android:layout_marginRight="16dp"
                android:background="@drawable/bg_grey_drawer_layout_2"
                android:elevation="@dimen/_5sdp"
                android:padding="@dimen/_5sdp"
                android:src="@drawable/icon_pos"
                android:tint="@color/orange" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_gravity="center_vertical"
                android:text="Address"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="@color/orange"
                android:textSize="@dimen/_15ssp"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/navigation_tables"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/_6sdp"
            android:layout_marginTop="@dimen/_4sdp"
            android:layout_marginBottom="@dimen/_4sdp"
            android:background="@drawable/bg_grey_drawer_layout_1"
            android:elevation="@dimen/_3sdp"
            android:orientation="horizontal"
            android:padding="@dimen/_7sdp"
            android:visibility="gone">

            <ImageButton
                android:layout_width="@dimen/_30sdp"
                android:layout_height="@dimen/_30sdp"
                android:layout_centerVertical="true"
                android:layout_marginRight="16dp"
                android:background="@drawable/bg_grey_drawer_layout_2"
                android:elevation="@dimen/_5sdp"
                android:padding="@dimen/_5sdp"
                android:src="@drawable/ic_delivery_status"
                android:tint="@color/orange" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_gravity="center_vertical"
                android:text="Status"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="@color/orange"
                android:textSize="@dimen/_15ssp"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/navigation_notifications"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/_6sdp"
            android:layout_marginTop="@dimen/_4sdp"
            android:layout_marginBottom="@dimen/_4sdp"
            android:background="@drawable/bg_grey_drawer_layout_1"
            android:elevation="@dimen/_3sdp"
            android:orientation="horizontal"
            android:padding="@dimen/_7sdp"
            android:visibility="gone">

            <ImageButton
                android:layout_width="@dimen/_30sdp"
                android:layout_height="@dimen/_30sdp"
                android:layout_centerVertical="true"
                android:layout_marginRight="16dp"
                android:background="@drawable/bg_grey_drawer_layout_2"
                android:elevation="@dimen/_5sdp"
                android:padding="@dimen/_5sdp"
                android:src="@drawable/ic_history"
                android:tint="@color/orange" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_gravity="center_vertical"
                android:text="History"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="@color/orange"
                android:textSize="@dimen/_15ssp"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/navigation_logout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:elevation="@dimen/_3sdp"
            android:background="@drawable/bg_grey_drawer_layout_1"
            android:layout_margin="@dimen/_6sdp"
            android:layout_marginTop="@dimen/_4sdp"
            android:layout_marginBottom="@dimen/_4sdp"
            android:padding="@dimen/_7sdp">

            <ImageButton
                android:layout_width="@dimen/_30sdp"
                android:layout_height="@dimen/_30sdp"
                android:layout_centerVertical="true"
                android:layout_marginRight="16dp"
                android:padding="@dimen/_5sdp"
                android:background="@drawable/bg_grey_drawer_layout_2"
                android:elevation="@dimen/_5sdp"
                android:tint="@color/orange"
                android:src="@drawable/ic_baseline_power_settings_new_24" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_gravity="center_vertical"
                android:text="@string/Logout"
                android:textStyle="bold"
                android:textSize="@dimen/_15ssp"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="@color/orange" />
        </LinearLayout>

        <!--<LinearLayout
            android:id="@+id/linearUserLogout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:elevation="@dimen/_3sdp"
            android:background="@color/white"
            android:layout_marginTop="@dimen/_4sdp"
            android:layout_marginBottom="@dimen/_4sdp"
            android:padding="@dimen/_7sdp">

            <ImageButton
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginRight="16dp"
                android:padding="@dimen/_5sdp"
                android:background="@color/white"
                android:elevation="@dimen/_5sdp"
                android:tint="@color/orange"
                android:src="@drawable/ic_exit" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_gravity="center_vertical"
                android:text="Logout"
                android:textStyle="bold"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="@color/orange" />
        </LinearLayout>-->

    </LinearLayout>

</LinearLayout>